+package com.javaspringatt.lanchonete.controller;
+import com.javaspringatt.lanchonete.models.AttPedidoModel;
+import com.javaspringatt.lanchonete.services.AttPedidoService;
+import org.springframework.web.bind.annotation.*;
+import java.util.List;
+
+@RestController
+@RequestMapping("/pedidos")
+public class AttPedidoController {
+
+  private final AttPedidoService service;
+
+  public AttPedidoController(AttPedidoService service) {
+    this.service = service;
+  }
+
+  @PostMapping
+  public AttPedidoModel criarPedido(@RequestBody AttPedidoModel pedido) {
+    return service.criarPedido(pedido);
+  }
+
+  @GetMapping
+  public List<AttPedidoModel> listar() {
+    return service.listar();
+  }
+
+  @GetMapping("/{id}")
+  public AttPedidoModel buscar(@PathVariable Long id) {
+    return service.buscar(id);
+  }
+}
